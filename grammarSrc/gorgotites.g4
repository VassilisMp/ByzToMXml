grammar gorgotites;

gorgotita : gorgon
        | gorgonParestigmenoAristera
        | gorgonParestigmenoDexia
        | digorgon
        | digorgonParestigmenoAristera
        | digorgonParestigmenoDexia
        | trigorgon
        | trigorgonParestigmenoAristeraKato
        | trigorgonParestigmenoAristeraPano
        | trigorgonParestigmenoDexia
        | ARGON
        | IMIDIARGON
        | DIARGON
        ;

gorgon : GORGON_NEO_ANO
        | GORGON_NEO_KATO
        | GORGON_USED_ON_KENTIMATA_ABOVE_OLIGON
        | GORGON_USED_ON_YPORROI
        | GORGON_USED_ON_L116_YPORROI
        | GORGON_ANO_STA_DEXIA
        | GORGON_KATO
        | GORGON_KATO_STA_DEXIA
        ;

gorgonParestigmenoAristera :
        GORGON_PARESTIGMENON_ARISTERA
        | GORGON_LEFT_DOT_USED_ON_L116_YPORROI
        | GORGON_PARESTIGMENON_ARISTERA_STA_DEXIA
        | GORGON_LEFT_DOT_USED_ON_YPORROI
        ;

gorgonParestigmenoDexia :
        GORGON_PARESTIGMENON_DEXIA
        | GORGON_RIGHT_DOT_USED_ON_YPORROI
        | GORGON_PARESTIGMENON_DEXIA_STA_DEXIA
        ;

digorgon :
        DIGORGON
        | DIGORGON_USED_ON_KENTIMATA_ABOVE_OLIGON
        | DIGORGON_USED_ON_YPORROI
        | DIGORGON_USED_ON_L116_YPORROI
        | DIGORGON_STA_DEXIA
        ;

digorgonParestigmenoAristera :
        DIGORGON_PARESTIGMENON_ARISTERA_KATO
        | DIGORGON_PARESTIGMENON_ARISTERA_ANO
        | DIGORGON_LEFT_DOT_USED_ON_YPORROI
        | DIGORGON_PARESTIGMENON_ARISTERA_KATO_STA_DEXIA
        | DIGORGON_PARESTIGMENON_ARISTERA_ANO_STA_DEXIA
        ;

digorgonParestigmenoDexia :
        DIGORGON_PARESTIGMENON_DEXIA
        | DIGORGON_PARESTIGMENON_DEXIA_STA_DEXIA
        ;

trigorgon :
        TRIGORGON
        | TRIGORGON_USED_ON_YPORROI
        | TRIGORGON_USED_ON_L116_YPORROI
        | TRIGORGON_STA_DEXIA
        ;

trigorgonParestigmenoAristeraKato :
        TRIGORGON_LEFT_DOT_USED_ON_YPORROI
        | TRIGORGON_PARESTIGMENON_ARISTERA_KATO
        | TRIGORGON_PARESTIGMENON_ARISTERA_KATO_STA_DEXIA
        ;

trigorgonParestigmenoAristeraPano :
        TRIGORGON_PARESTIGMENON_ARISTERA_ANO
        | TRIGORGON_PARESTIGMENON_ARISTERA_ANO_STA_DEXIA
        ;

trigorgonParestigmenoDexia :
        TRIGORGON_PARESTIGMENON_DEXIA
        | TRIGORGON_PARESTIGMENON_DEXIA_STA_DEXIA
        ;

