        | KENTIMATA_ABOVE_OLIGON # kentimataOnOligon
        | KENTIMATA_ABOVE_OLIGON KENTIMA_NEO_MESO # kentimataOnOligonAndKentima
        | APLI_UNDER_ANTIKENOMA_UNDER_OLIGON_ABOVE_KENTIMATA # oligonOnKentimataAndApli
        ;


